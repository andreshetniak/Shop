@model ShopCartViewModel

<div class="container">
		@foreach(var item in Model.ShopCart.ListShopItems)
		 {
			<div class="alert alert-warning mt-3">
				<b>Auto: </b> @item.Car.Name<br/>
				<b>Price: </b> @item.Car.Price.ToString("c");
			</div>
		 }
	<hr/>
	<div class="btn btn-danger" asp-controller="Order">Pay</div>
</div>